{
  "name": "optimize train",
  "description": "通勤時間を効率化するプラグイン",
  "manifest_version": 2,
  "version": "0.1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "通勤時間",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  },
  "background": {
    "scripts": ["jquery.min.js", "script.js"],
    "persistent": false
  },
  "permissions": [
    "alarms",
    "http://transit.loco.yahoo.co.jp/"
  ]
}